@{
    ViewBag.Title = "Home Page";
}
<style>
    /* Always set the map height explicitly to define the size of the div
      * element that contains the map. */
    #map {
        height: 400px;
        width:700px;
    }
  
</style>
<br />

<div id="map"></div>
<script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 11.016924, lng: 76.986482 },
          zoom: 8
        });
      }
</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyACf56q3-Qz85rv1FevdQUR5ENrMLLqVf4&callback=initMap">
</script>

<div class="row"> 
</div>
